{
    "name": "antick/skyii-user",
    "description": "User management for Yii2 ",
    "keywords": ["yii2", "admin", "auth", "rbac"],
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Pankaj Sanam",
            "email": "pankaj.sanam@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.12"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "modules\\user\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
